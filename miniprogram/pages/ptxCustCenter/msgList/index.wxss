.libBox {
  /* height: 700rpx; */
  text-align: left;
  overflow-y: scroll;
}
.libBox .l-left {
  margin-left: 5px;
  position: relative;
  width: 75%;
  margin-bottom: 15px;
}
.libBox .l-left .l-inner {
  margin-left: 50px;
  background: #eee;
  color: #000;
  padding: 9px 13px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
  position: relative;
}
.libBox .l-left .l-inner::before {
  content: "";
  top: 14px;
  border: 6px solid transparent;
  position: absolute;
  left: -9px;
  border-right-color: #eee;
  border-right-width: 4px;
}
.libBox .l-left::before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 40px;
  height: 40px;
  background: url(data:image/jpeg;base64,);
  background-repeat: no-repeat;
  background-size: contain;
}
.libBox .l-right {
  padding: 9px 13px;
  width: 93%;
  text-align: left;
  margin-bottom: 15px;
  position: relative;
}
.libBox .l-right .l-inner {
  background: #b2e281;
  margin-right: 40px;
  margin-left: 42px;
  color: #000;
  padding: 9px 13px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
  position: relative;
}
.libBox .l-right .l-inner::before {
  content: "";
  top: 14px;
  border: 6px solid transparent;
  position: absolute;
  right: -10px;
  border-left-color: #b2e281;
  border-left-width: 4px;
}
.libBox .l-right::after {
  content: "";
  position: absolute;
  right: 5px;
  top: 10px;
  width: 40px;
  height: 40px;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: contain;
}
.sendBox {
  text-align: right;
  color: #888;
  padding: 10px 5px;
}
.sendBox textarea{
  margin: 0 auto;
  width: 95%;
  padding: 10rpx;
  text-align: left;
  overflow-y:scroll; 
  border:1rpx solid #eee;
  
  height: 150rpx;
  margin-bottom: 10rpx;
}
.sendBox span {
  margin-left: 10px;
  display: inline-block;
  border: 1px solid #c1c1c1;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  padding: 3px 20px;
  font-size: 14px;
  background-color: #fff;
  color: #222;
  padding-left: 30px;
  padding-right: 30px;
}
.enterBox{
  width: 100%;
  position: fixed;
  bottom: 0;
}
.contentBox{
   padding-top: 20rpx;
}
.welcomContent{
    text-align: center;
    padding: 10px 0;
    line-height: 20px;
    color: #a5a5a5;
}
.timeShow{
  color: #a5a5a5;
  font-size: 24rpx;
  text-align: center;
}

